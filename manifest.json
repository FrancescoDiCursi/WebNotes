{
	"manifest_version" : 3,
	"name" : "iNotes",
	"version" : "0.001",
	"permissions":[
        "webRequest",
        "scripting",
        "activeTab",
        "clipboardRead",
        "clipboardWrite",
        "tabs",
        "storage"
    ],
    "host_permissions":[
        "<all_urls>"
    ],
    "background":{
        "service_worker":"background.js"
    }
    ,
    "action" : {
        "default_icon":"icon.jpg",
        "default_popup":"popup.html"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
    ,
    "icons":{
        "16":"icon.jpg",
        "48":"icon.jpg",
        "128":"icon.jpg"
    },
    "description": "Interacive web nodes"
    
}